#include <stdlib.h>

int* plusOne(int* digits, int digitsSize, int* returnSize) {
    int carry = 0; 
    int size;
    int* result = (int*)malloc((digitsSize + 1) * sizeof(int)); 

    size = digitsSize;

   
    if (digits[size - 1] == 9)
    {
        result[size - 1] = 0;
        result[size -2] = digits[size - 1];
        result[size -2]++;
    }
    else
    {
        result[size -1] = digits[size - 1];
        result[size - 1]++;
    }
    return result;
}
